{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module",
        "impliedStrict": true
    },
    "plugins": [
        "react"
    ],
    "extends": [
        "airbnb"
    ],
    "rules": {
        "no-console": 0,
        "func-names": 0,
        "object-curly-spacing": 0,
        "comma-dangle": 0,
        "arrow-body-style": 0,
        "consistent-return": 0,
        "no-param-reassign": "warn",
        "arrow-parens": 0,
        "react/jsx-no-bind": 0,
        "semi": [
            "error",
            "never"
        ]
    },
    "env": {
        "browser": true,
        "node": true
    },
    "globals": {}
}